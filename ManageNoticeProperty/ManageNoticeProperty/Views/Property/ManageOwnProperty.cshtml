@model IEnumerable<ManageNoticeProperty.Models.Flat>
@{
    ViewBag.Title = "ManageOwnProperty";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Zarządzanie ofertami</h2>
@if (Model.Count() > 0)
{
    foreach (var flat in Model)
    {
        <div class="row">
            <a href="@Url.Action("EditProperty", "Property", new { id = flat.FlatId })"> @flat.City</a>
        </div>
        <br />

    }
}
else
{
    <div class="row">
        Brak dodanych ofert
    </div>
}

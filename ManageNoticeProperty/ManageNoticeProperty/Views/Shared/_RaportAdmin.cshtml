@model ManageNoticeProperty.ViewModel.AdminRaportViewModel
<div id="property-list" class="row">
    <div class="col-lg-12">
        <table class="table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(x => x.Order.FirstOrDefault().Flat.City)
                    </th>
                    <th>
                        @Html.DisplayNameFor(x => x.Order.First().Flat.Street)
                    </th>
                    <th>
                        @Html.DisplayNameFor(x => x.Order.First().Flat.PostCode)
                    </th>
                    <th>
                        @Html.DisplayNameFor(x => x.Order.First().SellDate)
                    </th>     
                    <th>
                        Sell user
                    </th>
                    <th>
                        Buy user
                    </th>
                    <th>
                        @Html.DisplayNameFor(x => x.Order.First().Flat.Price)
                    </th>
                </tr>

            </thead>
            @foreach (var order in Model.Order)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(x => order.Flat.City)
                    </td>
                    <td>
                        @Html.DisplayFor(x => order.Flat.Street)
                    </td>
                    <td>
                        @Html.DisplayFor(x => order.Flat.PostCode)
                    </td>
                    <td>
                        @Html.DisplayFor(x => order.SellDate)
                    </td>
                    <td>
                        @Html.DisplayFor(x => order.Flat.User.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(x => order.BuyUser.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(x => order.Price)
                    </td>
                </tr>

            }
            <tfoot>
                <tr>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td>Razem:</td>
                    <td>@Model.TotalPrice Do EDYCJI!! zastanowienia</td>
                </tr>
            </tfoot>
        </table>

    </div>
</div>
